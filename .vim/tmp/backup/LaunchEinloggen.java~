package de.hs_mannheim.launcher;

import de.hs_mannheim.ui.Einlog;
import de.hs_mannheim.ui.UIController;
import user.UserModellForEinlog;

public class LaunchEinloggen {

	public LaunchEinloggen() {
		firedUI();
	}
	
	private void firedUI(){
			UserModellForEinlog model = new UserModellForEinlog(); 
			Einlog einlog = new Einlog(); 
			UIController controller = new UIController(einlog, model); 
			einlog.pack(); 
			einlog.setSize(600, 400);
	}
}
