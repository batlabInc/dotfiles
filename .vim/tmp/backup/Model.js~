
/**
 * Class : ModelUser : 
 * properties : -name 
 *              -vorname 
 *  
 * */
var ModelUser = function(){}; 
ModelUser.prototype.name = ""; 
ModelUser.prototype.vorname=""; 

ModelUser.prototype.getName     = function(){ return this.name; }; 
ModelUser.prototype.getVorname  = function(){ return this.vorname; }; 
ModelUser.prototype.toString    = function(){ return this.name + this.vorname; }; 




var foo = new ModelUser(); 
foo.name="Fabrice"; 
foo.vorname="siyapdje"; 

