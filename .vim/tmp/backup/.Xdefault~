URxvt.font: xft:Terminus:pixelsize=14:lang=af,xft:Fixed:SemiCondensed:pixelsize=13:lang=cs,xft:DejaVu Sans:pixelsize=10

URxvt.saveLines: 12000
URxvt.scrollBar: false
URxvt.urgentOnBell: true

! perl extensions
URxvt.perl-ext: default,url-select,clipboard

! for yankable URLs

URxvt.keysym.M-y: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/firefox -new-tab
URxvt.url-select.button: -

! for using the clipboard

URxvt.keysym.4-c: perl:clipboard:copy
URxvt.keysym.4-v: perl:clipboard:paste

