function swipswap(){
 
        if(document.getElementById('registrationButton').onclick)
        {   
         document.getElementById('einlogFormular').style.display = 'none';  
         document.getElementById('RegistrationFormular').style.display = 'block'; 
        } 
}

function swapswip()
{

if(document.getElementById('einlogButton').onclick)
{
    document.getElementById('RegistrationFormular').style.display = 'none'; 
    document.getElementById('einlogFormular').style.display = 'block'; 
}

}












