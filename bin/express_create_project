#!/bin/sh 


#Get node project verzeichniss 
node_verzeichniss="$HOME/Project/JS/NODE/" 

echo "$node_verzeichniss$1"

express --ejs "$node_verzeichniss$1"

cd "$node_verzeichniss$1" && npm install 

mkdir "src"

mkdir "src/db"
mkdir "src/db/MOD"
mkdir "src/db/SCH"

mkdir "src/ctrl/CTL/"

mkdir "src/utils/"
mkdir "src/utils/UTILS"

git init 

